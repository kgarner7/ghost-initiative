{
  "presets": [
    [
      "@babel/env", {
        "corejs": 3,
        "useBuiltIns": "entry"
      }
    ],
    ["@babel/typescript", { "jsxPragma": "h" }]
  ],
  "plugins": [
    ["@babel/plugin-transform-react-jsx", {
      "pragma": "h"
    }], ["import", {
      "libraryName": "antd",
      "libraryDirectory": "es",
      "style": true
    }]
  ]
}